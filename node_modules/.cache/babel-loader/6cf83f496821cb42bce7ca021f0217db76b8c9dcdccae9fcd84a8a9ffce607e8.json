{"ast":null,"code":"export default {\n  props: {\n    manager: Object,\n    filterAgeValue: String\n  },\n  computed: {\n    address() {\n      return this.manager.address.suite + \" \" + this.manager.address.street + \", \" + this.manager.address.zipcode + \" \" + this.manager.address.city + \".\";\n    },\n    ageMax() {\n      return this.filterAgeValue === \"20andBelow\" ? 20 : this.filterAgeValue === \"21to39\" ? 39 : Math.min(); //Infinity\n    },\n\n    ageMin() {\n      return this.filterAgeValue === \"21to39\" ? 21 : this.filterAgeValue === \"40andAbove\" ? 40 : Math.max(); //-Infinity\n    }\n  }\n};","map":{"version":3,"names":["props","manager","Object","filterAgeValue","String","computed","address","suite","street","zipcode","city","ageMax","Math","min","ageMin","max"],"sources":["C:\\Users\\simkayee\\Documents\\1 Work Code\\Vuejs tutorial\\first-week-task\\src\\components\\ManagerList.vue"],"sourcesContent":["<template>\r\n   <div\r\n      class=\"manager-card\"\r\n      v-show=\"manager.age <= ageMax && manager.age >= ageMin\"\r\n   >\r\n      <img src=\"../assets/userphoto-prime1.jpg\" class=\"manager-image\" />\r\n      <div class=\"manager-info\">\r\n         <div class=\"manager-name\">{{ manager.name }}</div>\r\n         <div class=\"manager-detail\">\r\n            Email: <span>{{ manager.email }}</span>\r\n         </div>\r\n         <div class=\"manager-detail\">\r\n            Mobile: <span>{{ manager.phone }}</span>\r\n         </div>\r\n         <div class=\"manager-detail\">\r\n            Company: <span>{{ manager.company }}</span>\r\n         </div>\r\n         <div class=\"manager-detail\">\r\n            Address: <span>{{ address }}</span>\r\n         </div>\r\n         <div class=\"manager-detail\">\r\n            Website: <span>{{ manager.website }}</span>\r\n         </div>\r\n         <div class=\"manager-detail\">\r\n            Age: <span>{{ manager.age }}</span>\r\n         </div>\r\n      </div>\r\n   </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n   props: {\r\n      manager: Object,\r\n      filterAgeValue: String,\r\n   },\r\n   computed: {\r\n      address() {\r\n         return (\r\n            this.manager.address.suite +\r\n            \" \" +\r\n            this.manager.address.street +\r\n            \", \" +\r\n            this.manager.address.zipcode +\r\n            \" \" +\r\n            this.manager.address.city +\r\n            \".\"\r\n         );\r\n      },\r\n      ageMax() {\r\n         return this.filterAgeValue === \"20andBelow\"\r\n            ? 20\r\n            : this.filterAgeValue === \"21to39\"\r\n            ? 39\r\n            : Math.min(); //Infinity\r\n      },\r\n      ageMin() {\r\n         return this.filterAgeValue === \"21to39\"\r\n            ? 21\r\n            : this.filterAgeValue === \"40andAbove\"\r\n            ? 40\r\n            : Math.max(); //-Infinity\r\n      },\r\n   },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.manager-card {\r\n   width: 29%;\r\n   margin-bottom: 3rem;\r\n   margin-right: 4.1%;\r\n   background: white;\r\n   box-shadow: 0px 0px 23px -12px rgba(0, 0, 0, 0.3);\r\n   display: flex;\r\n   flex-direction: row;\r\n}\r\n\r\n.manager-image {\r\n   width: 85px;\r\n   height: 85px;\r\n   margin: 2.5rem 1.7rem 0 2rem;\r\n   border-radius: 50%;\r\n}\r\n\r\n.manager-info {\r\n   margin: 2.5rem 1.5rem 3.3rem 0;\r\n}\r\n\r\n.manager-name {\r\n   font-size: 1.5rem;\r\n   font-weight: 700;\r\n   text-transform: uppercase;\r\n   margin-bottom: 0.9rem;\r\n}\r\n\r\n.manager-detail span {\r\n   font-weight: 600;\r\n}\r\n\r\n@media screen and (min-width: 4000px) {\r\n   .manager-card {\r\n      width: 20.7%;\r\n   }\r\n}\r\n\r\n@media screen and (max-width: 1800px) {\r\n   .manager-card {\r\n      width: 45.6%;\r\n   }\r\n}\r\n\r\n@media screen and (max-width: 1155px) {\r\n   .manager-card {\r\n      width: 95%;\r\n   }\r\n}\r\n\r\n@media screen and (max-width: 500px) {\r\n   .manager-card {\r\n      flex-direction: column;\r\n      min-width: fit-content;\r\n   }\r\n\r\n   .manager-info {\r\n      margin-left: 2rem;\r\n   }\r\n}\r\n</style>\r\n"],"mappings":"AA+BA,eAAe;EACZA,KAAK,EAAE;IACJC,OAAO,EAAEC,MAAM;IACfC,cAAc,EAAEC;EACnB,CAAC;EACDC,QAAQ,EAAE;IACPC,OAAOA,CAAA,EAAG;MACP,OACG,IAAI,CAACL,OAAO,CAACK,OAAO,CAACC,KAAI,GACzB,GAAE,GACF,IAAI,CAACN,OAAO,CAACK,OAAO,CAACE,MAAK,GAC1B,IAAG,GACH,IAAI,CAACP,OAAO,CAACK,OAAO,CAACG,OAAM,GAC3B,GAAE,GACF,IAAI,CAACR,OAAO,CAACK,OAAO,CAACI,IAAG,GACxB,GAAE;IAER,CAAC;IACDC,MAAMA,CAAA,EAAG;MACN,OAAO,IAAI,CAACR,cAAa,KAAM,YAAW,GACrC,EAAC,GACD,IAAI,CAACA,cAAa,KAAM,QAAO,GAC/B,EAAC,GACDS,IAAI,CAACC,GAAG,CAAC,CAAC,EAAE;IACpB,CAAC;;IACDC,MAAMA,CAAA,EAAG;MACN,OAAO,IAAI,CAACX,cAAa,KAAM,QAAO,GACjC,EAAC,GACD,IAAI,CAACA,cAAa,KAAM,YAAW,GACnC,EAAC,GACDS,IAAI,CAACG,GAAG,CAAC,CAAC,EAAE;IACpB;EACH;AACH,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}